{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": [
      "es6",
      "DOM"
    ],
    "skipLibCheck": true,
    "allowJs": true,
    "jsx": "react",
    "strict": true,
    "noImplicitAny": true,
    "baseUrl": "./",
    "paths": {
      "~*": [
        "src*"
      ],
    },
    "noEmit": true
  }
}